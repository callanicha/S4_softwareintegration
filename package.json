    "@types/chai": "^5.2.0",
    "@types/cors": "^2.8.17",
    "@types/jest": "^29.5.14",
    "@types/mocha": "^10.0.10",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "eslint-plugin-jest": "^28.11.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "mocha": "^9.2.2",
    "nyc": "^15.1.0",
    "rewire": "^6.0.0",
    "sinon": "^13.0.1",
    "sinon-chai": "^3.7.0",
    "supertest": "^6.2.2",
    "ts-jest": "^29.2.6",
    "ts-mocha": "^11.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "@types/express": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^8.27.0",
    "@typescript-eslint/parser": "^8.27.0",
    "cors": "^2.8.5",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "express": "^4.17.3",
    "mongoose": "^6.2.6",
    "nanoid": "^3.3.1",
    "prettier": "^3.5.3",
    "typescript-eslint": "^8.27.0"
  },
  "repository": {
    "type": "git",
